{
  "name": "@adedayomatthews/proxy-manager",
  "version": "1.0.18",
  "author": "Adedayo Matthews",
  "scripts": {
    "presandbox": "npm run sequelize-sync",
    "sandbox": "nodemon ./sandbox/bin/www",
    "sequelize-model": "node src/sync-model.js",
    "sequelize-migrate": "npx sequelize-cli db:migrate",
    "sequelize-seed": "npx sequelize-cli db:seed:all",
    "sequelize-sync": "npm run sequelize-migrate && npm run sequelize-model && npm run sequelize-seed"
  },
  "dependencies": {
    "@adedayomatthews/swagger-generator": "^1.0.0",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "jade": "1.11.0",
    "mysql2": "^3.6.0",
    "sequelize": "^6.32.1",
    "sequelize-cli": "6.6.0"
  },
  "devDependencies": {
    "nodemon": "3.0.1"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adedayomatt/proxy-manager.git"
  },
  "keywords": [
    "api",
    "swagger",
    "documentation",
    "automation",
    "proxy",
    "server",
    "express"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adedayomatt/proxy-manager/issues"
  },
  "homepage": "https://github.com/adedayomatt/proxy-manager#readme",
  "description": "Manager for api calls to multiple third party"
}
